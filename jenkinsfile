pipeline {
    agent any
    tools{
        maven 'mymaven'
        jdk 'myjava'
    }
    stages{
        stage('compile'){
            agent any
            steps{
                git 'https://github.com/abhicse2013/project2.git'
                sh "mvn compile"
            }
        }
        stage('test'){
            steps{
                sh "mvn test"
            }
    }
        stage('Build'){
            steps{
                sh "mvn package"
            }
    }

    }
}
