pipeline {
    agent any
    tools{
        maven 'mymaven'
    }
    stages{
        stage('Build Maven'){
            steps{
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/abhicse2013/project2']]])
                sh 'mvn clean install'
            }
        }

    }
}
